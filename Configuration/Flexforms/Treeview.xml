<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <apikey>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.apikey</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </apikey>
                    <dataFormatter>
                        <TCEforms>
                            <onChange>reload</onChange>
                            <displayCond>FIELD:pages:REQ:true</displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.data_formatter</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Signatur Hierarchie</numIndex>
                                        <numIndex index="1">signature</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Kollektionen gruppiert nach Jahreszahlen</numIndex>
                                        <numIndex index="1">collection</numIndex>
                                    </numIndex>
                                </items>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </dataFormatter>
                    <field>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.field</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </field>
                    <yearFrom>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.yearFrom</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>4</max>
                                <eval>int,trim,required</eval>
                            </config>
                        </TCEforms>
                    </yearFrom>
                    <yearTill>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.yearTill</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>4</max>
                                <eval>int,trim,required</eval>
                            </config>
                        </TCEforms>
                    </yearTill>
                    <yearRange>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.yearRange</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </yearRange>
                    <regexOutput>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.regexOutput</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                                <max>100</max>
                                <eval>trim,required</eval>
                                <default>/\[([0-9]*) TO ([0-9]*)\]/</default>
                            </config>
                        </TCEforms>
                    </regexOutput>
                    <regexOutputDelimiter>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.regexOutputDelimiter</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>10</max>
                                <eval>trim</eval>
                                <default>-</default>
                            </config>
                        </TCEforms>
                    </regexOutputDelimiter>
                    <leafFieldOutput>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.leafFieldOutput</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </leafFieldOutput>
                    <sorting>
                        <TCEforms>
                            <displayCond>FIELD:pages:REQ:true</displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.sorting</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Aufsteigend</numIndex>
                                        <numIndex index="1">asc</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Absteigend</numIndex>
                                        <numIndex index="1">desc</numIndex>
                                    </numIndex>
                                </items>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </sorting>
                    <explode>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:signature</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.explode</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </explode>
                    <removeFromTitle>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:signature</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.removeFromTitle</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </removeFromTitle>
                    <queryPrefix>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:signature</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.queryPrefix</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>30</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </queryPrefix>
                    <dataCollections>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.collections</label>
                            <config>
                                <type>select</type>
                                <items type="array"/>
                                <itemsProcFunc>Kitodo\Dlf\Hooks\FormEngine->itemsProcFunc_collectionList</itemsProcFunc>
                                <size>5</size>
                                <autoSizeMax>15</autoSizeMax>
                                <maxitems>1024</maxitems>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </dataCollections>
                    <pages>
                        <TCEforms>
                            <onChange>reload</onChange>
                            <exclude>1</exclude>
                            <label>LLL:EXT:lang/locallang_general.xml:LGL.startingpoint</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </pages>
                    <solrcore>
                        <TCEforms>
                            <displayCond>FIELD:pages:REQ:true</displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.solrcore</label>
                            <config>
                                <type>select</type>
                                <items type="array"></items>
                                <itemsProcFunc>Kitodo\Dlf\Hooks\FormEngine->itemsProcFunc_solrList</itemsProcFunc>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </solrcore>
                    <collections>
                        <TCEforms>
                            <displayCond>
                                <AND>
                                    <numIndex index="0">FIELD:pages:REQ:true</numIndex>
                                    <numIndex index="1">FIELD:dataFormatter:!=:collection</numIndex>
                                </AND>
                            </displayCond>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.collections</label>
                            <config>
                                <type>select</type>
                                <items type="array"/>
                                <itemsProcFunc>Kitodo\Dlf\Hooks\FormEngine->itemsProcFunc_collectionList</itemsProcFunc>
                                <size>5</size>
                                <autoSizeMax>15</autoSizeMax>
                                <maxitems>1024</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </collections>
                    <templateFile>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dlf/Resources/Private/Language/Treeview.xml:tt_content.pi_flexform.templateFile</label>
                            <config>
                                <type>group</type>
                                <internal_type>file_reference</internal_type>
                                <allowed>tmpl,tpl,html,htm,txt</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <disable_controls>upload</disable_controls>
                            </config>
                        </TCEforms>
                    </templateFile>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
